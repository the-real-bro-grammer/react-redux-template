{
  "singleQuote": true,
  "printWidth": 120,
  "editor.formatOnSave": true,
  "endOfLine": "auto",
  "proseWrap": "always",
  "tabWidth": 4,
  "requireConfig": false,
  "useTabs": false,
  "sortingMethod": "alphabetical",
  "importOrder": ["^components/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "arrowParens": "avoid"
}
